{
    "name": "reallyli/laravel-unideploy",
    "description": "Custom Unique Laravel Deployer",
    "keywords": ["deployer", "laravel", "deploy", "reallyli", "laravel-deploy"],
    "license": "MIT",
    "authors": [
        {
            "name": "reallyli",
            "email": "skrphper@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "deployer/deployer": "^6.1",
        "deployer/recipes": "^6.0",
        "illuminate/support": "^5.5 || ^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
       "psr-4": {
            "Reallyli\\LaravelDeployer\\": "src/LaravelDeployer"
        },
        "files": [
            "src/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Reallyli\\LaravelDeployer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reallyli\\LaravelDeployer\\LaravelDeployerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
